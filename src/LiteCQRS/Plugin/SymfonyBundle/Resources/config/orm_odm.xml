<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="litecqrs.orm_odm.identity_map.class">LiteCQRS\ORMODMIdentityMap</parameter>
    </parameters>

    <services>
        <service id="litecqrs.identity_map.orm_odm" class="%litecqrs.orm_odm.identity_map.class%" public="false">
            <argument type="service" id="litecqrs.identity_map.orm" />
            <argument type="service" id="litecqrs.identity_map.odm" />
        </service>
    </services>
</container>
